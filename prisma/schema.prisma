generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Elon {
  id          String  @id @default(uuid())
  username    String?
  phone       String?
  second_phne String?
  from        String?
  to          String?
}

model Region {
  id        String      @id @default(uuid())
  name      String
  districts Districts[]
}

model Districts {
  id       String  @id @default(uuid())
  name     String
  region   Region? @relation(fields: [regionId], references: [id])
  regionId String?
}
