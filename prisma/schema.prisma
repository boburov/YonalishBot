generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Direction {
  id   String @id @default(uuid())
  from String
  to   String

  channels Channel[]

  @@unique([from, to])
  @@index([from])
  @@index([to])
}

model Channel {
  id          String    @id @default(uuid())
  channelId   String
  directionId String
  direction   Direction @relation(fields: [directionId], references: [id])

  @@unique([directionId, channelId])
}
